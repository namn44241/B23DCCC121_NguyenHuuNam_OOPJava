package hoc_java;

public class b5_kieu_dulieu {

	public static void main(String[] args) {
		System.out.println("min của int =" + Integer.MIN_VALUE);
		System.out.println("min của int =" + Integer.MAX_VALUE);
		System.out.println("min của int =" + Long.MIN_VALUE);
		System.out.println("min của int =" + Long.MAX_VALUE);
		System.out.println("min của int =" + Double.MIN_VALUE);
		System.out.println("min của int =" + Double.MAX_VALUE);

		boolean b = true;
		float f = 1.5f;
		//số thực cần chữ f để định dạng số tp
		double d = 1.5;

		
	}

}
